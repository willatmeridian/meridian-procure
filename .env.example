# Sanity CMS Configuration
# Copy this file to .env and fill in your actual values

# Your Sanity project ID (found in your Sanity dashboard)
SANITY_PROJECT_ID=your-project-id-here

# Your Sanity dataset (usually 'production')
SANITY_DATASET=production

# Sanity API Token (optional, for write operations)
SANITY_API_TOKEN=your-api-token-here

# HubSpot Configuration
# Your HubSpot Portal ID (found in HubSpot settings)
PUBLIC_HUBSPOT_PORTAL_ID=your-portal-id-here

# Your HubSpot Form ID (this form will be used for all contact forms on the site)
PUBLIC_HUBSPOT_FORM_ID=your-form-id-here